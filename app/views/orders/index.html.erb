<h1>Orders#index</h1>
<p>Find me in app/views/orders/index.html.erb</p>

<h2>All Orders</h2>

<% @orders.each do |order| %>
  <p>Name: <%= order.name %></p>
  <p>Email: <%= order.email %></p>
  <p>Line Items: </p>
  <ul>
    <% order.cart_items.each do |item| %>
    <li><%= item.quantity %> x <%= item.cab_config.id %></li>
    <% end %>
  </ul>
<% end
